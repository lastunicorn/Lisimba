<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Microsoft.Practices.Unity.Configuration"/>
    </configSections>

    <unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
        <alias alias="IGate" type="DustInTheWind.Lisimba.Egg.IGate, DustInTheWind.Lisimba.Egg" />
        <alias alias="ZipXmlGate" type="DustInTheWind.Lisimba.Gating.ZipXmlGate, DustInTheWind.Lisimba.ZipXmlGate" />
        <alias alias="YahooCsvGate" type="DustInTheWind.Lisimba.Gating.YahooCsvGate, DustInTheWind.Lisimba.YahooGate" />

        <container>

            <register type="IGate" mapTo="ZipXmlGate" name="ZipXmlGate"/>
            <register type="IGate" mapTo="YahooCsvGate" name="YahooCsvGate"/>

        </container>

    </unity>
</configuration>